#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d\n",&n);
    int a[n],i;
    for(i=0;i<n;i++)
    {
        scanf("%d ",&a[i]);
        if(a[i]%2==0)
            a[i]=0;
        else
            a[i]=1;
    }
    for(i=0;i<n;i++)
        printf("%d ",a[i]);
}
