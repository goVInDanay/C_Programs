#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d\n",&n);
    int a[n],min=0,max=0;
    for(int i=0;i<n;i++)
        scanf("%d ",&a[i]);
    min=a[0];
    max=a[0];
    for(int i=0;i<n;i++)
    {
        if(min>a[i])
            min=a[i];
        if(max<a[i])
            max=a[i];
    }
    printf("%d\n",min);
    printf("%d",max);
}
