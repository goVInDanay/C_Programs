#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n;
    scanf("%d",&n);
    printf("%d\n",n);
    printf("%o\n",n);
    printf("%x\n",n);
    printf("%X",n);
    return 0;
}
