#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,m,d=0,r=0;
    scanf("%d",&n);
    m=n;
    while(n!=0)
    {
        d=n%10;
        r=r*10+d;
        n=n/10;
    }
    if(r==m)
        printf("Palindrome.");
    else
        printf("Not Palindrome.");
}
