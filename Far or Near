#include <stdio.h>
#include <math.h>
int main(void) {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int x1,y1,x2,y2;
	    scanf("%d%d%d%d",&x1,&y1,&x2,&y2);
	    if(sqrt(x1*x1+y1*y1)>sqrt(x2*x2+y2*y2))
	    printf("ALEX\n");
	    else if(sqrt(x1*x1+y1*y1)<sqrt(x2*x2+y2*y2))
	    printf("BOB\n");
	    else
	    printf("EQUAL\n");
	}
	return 0;
}

