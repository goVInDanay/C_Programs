#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i;
    double f=1,sum=0.0;
    scanf("%d\n",&n);
    if(n>0&&n<10000)
    {
        for (i = 1; i <= n; i++)
    {
        f = f * i;
        sum = sum +(i / f);
    }
        printf("%lf",sum);
    }
}
