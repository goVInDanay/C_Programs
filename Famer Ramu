#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int n,x,y;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        scanf("%d %d",&x,&y);
        
        for(int j=1+x+y;;j++)
        {int c=0;
         for(int k=1;k<=j;k++)
         {
            if(j%k==0)
                c++;
        }
        
        if(c==2)
        {
            printf("%d",j);
            break;
        }
    }
}
}

